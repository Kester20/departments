<custom-sql>
    <sql id="com.aimprosoft.noormal.servicebuilder.service.persistence.EmployeeFinderImpl.findByDepartment">

        <![CDATA[
                SELECT * FROM employee WHERE employee.department = ?
            ]]>
    </sql>

    <sql id="com.aimprosoft.noormal.servicebuilder.service.persistence.EmployeeFinderImpl.getCountByDepartment">

